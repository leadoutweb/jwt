{
  "name": "leadout/jwt",
  "type": "library",
  "description": "Simple JWT authentication for Laravel.",
  "keywords": [
    "auth",
    "authentication",
    "guard",
    "json web token",
    "jwt",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "nesbot/carbon": "^3.0",
    "illuminate/http": "^12.0",
    "illuminate/auth": "^12.0",
    "firebase/php-jwt": "^6.0",
    "illuminate/cache": "^12.0",
    "illuminate/support": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Leadout\\JWT\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.4",
    "laravel/pint": "^1.13"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Leadout\\JWT\\JWTServiceProvider"
      ]
    }
  }
}
